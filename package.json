{
  "name": "worktop-qwikcity",
  "description": "worktop-qwikcity app cloudflare publish test",
  "engines": {
    "node": ">=16.0.0"
  },
  "private": false,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build --ssr src/entry.cloudflare-pages.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.12.1",
    "@builder.io/qwik-city": "0.0.118",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.27.0",
    "eslint-plugin-qwik": "0.12.1",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "vite-tsconfig-paths": "3.5.2",
    "wrangler": "2.1.15"
  }
}
